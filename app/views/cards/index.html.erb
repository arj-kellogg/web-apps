<%
    ranks = [2,3,4,5,6,7,8,9, "jack", "queen","king","ace"]
    suits = ["clubs","diamonds","hearts","spades"]

deck = []
for rank in ranks
    for suit in suits
        deck << "#{rank}_of_#{suit}"
    end
end

shuffled_deck= deck.shuffle
%>




<ul>
<li> <%=shuffled_deck[0] %> </li>
<img src="/images/cards/<%= shuffled_deck[0] %>.svg"